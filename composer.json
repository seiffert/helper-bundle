{
    "name": "seiffert/helper-bundle",
    "description": "This bundle provides a structure for helper objects.",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1.0,<2.3-dev"
    },
    "require-dev": {
        "phpmd/phpmd": "1.4.0",
        "pdepend/pdepend": "1.1.0",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-code-coverage": "1.2.*",
        "phpunit/php-timer": "1.0.4"
    },
    "autoload": {
        "psr-0": {
            "Seiffert\\HelperBundle": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Seiffert/HelperBundle"
}
